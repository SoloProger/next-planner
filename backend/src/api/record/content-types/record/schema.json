{
  "kind": "collectionType",
  "collectionName": "records",
  "info": {
    "singularName": "record",
    "pluralName": "records",
    "displayName": "Record",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "count": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "recordType": {
      "type": "enumeration",
      "enum": [
        "INCOME",
        "CONSUMPTION"
      ],
      "default": "CONSUMPTION",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "records"
    }
  }
}
